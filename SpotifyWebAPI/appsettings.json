{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Spotify": {
    "BaseUrlAuth": "https://accounts.spotify.com",
    "BaseUrlAPI": "https://api.spotify.com",
    "ClientID": "YOUR_CLIENT_ID",
    "ClientSecret": "YOUR_CLIENT_SECRET",
    "RedirectUri": "/api/v1/auth/callback",
    "Resources": {
      "Authorization": {
        "GrantType": "authorization_code",
        "TokenType": "Basic",
        "Uri": "/api/token"
      }
    }
  }
}